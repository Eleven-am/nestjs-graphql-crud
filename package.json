    "push": "npm version patch && npm run copy && cd dist && npm publish && cd ..",
    "pipeline": "npm run build && npm run push"
  },
  "keywords": [
    "nestjs",
    "graphql",
    "crud",
    "api",
    "nestjs-graphql-crud",
    "nestjs-graphql",
    "nestjs-crud"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Roy OSSAI",
  "main": "./index.js",
  "types": "./index.d.ts",
  "license": "GPL-3.0",
